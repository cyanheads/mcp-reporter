{
  "mcpServers": {
    "atlas-mcp-server": {
      "command": "/path/to/node",
      "args": [
        "/path/to/atlas-mcp-server/dist/index.js"
      ],
      "env": {
        "NEO4J_URI": "bolt://localhost:7687",
        "NEO4J_USER": "neo4j",
        "NEO4J_PASSWORD": "your-password-here"
      }
    }
  }
}